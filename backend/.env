# MongoDB connection string
MONGO_URI=mongodb://localhost:27017/Project
# JWT secret key
JWT_SECRET=your_jwt_secret_key

# Email config for nodemailer (Gmail example)
EMAIL_USER=your_gmail@gmail.com
EMAIL_PASS=your_gmail_app_password

# VNPay config - using sandbox test values
VNP_TMNCODE=CODESOURCE
VNP_HASHSECRET=PPPPPPPPPPPPPPPPPPPPPPPPP
VNP_URL=https://sandbox.vnpayment.vn/paymentv2/vpcpay.html
# URL callback sẽ được tạo động dựa trên host của request
# Để trống để sử dụng cơ chế tự động, hoặc điền URL chính xác của bạn
VNP_RETURN_URL=

# Server port (optional, default 9999)
PORT=9999